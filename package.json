{
  "name": "web2js",
  "version": "1.0.0",
  "description": "Convert \"Pascal\" output of WEB into JavaScript",
  "main": "index.js",
  "scripts": {
    "test": "node spec/run-tests.js",
    "get-tex-sources": "wget -N http://ctan.math.washington.edu/tex-archive/systems/knuth/dist/tex/tex.web && wget -O etex.ch 'https://tug.org/svn/texlive/trunk/Build/source/texk/web2c/etexdir/etex.ch?revision=32727&view=co'",
    "build": "jison parser.jison",
    "generate-wasm": "tie -m etex.web tex.web etex.ch && tangle -underline etex.web etex.sys && node compile.js etex.p",
    "initex": "node initex.js"
  },
  "author": "Jim Fowler",
  "license": "GPL-3.0",
  "dependencies": {
    "binaryen": "^64.0.0",
    "flex-js": "^1.0.4",
    "jison": "^0.4.18",
    "pegjs": "^0.10.0"
  },
  "devDependencies": {
    "colors": "^1.3.3",
    "glob": "^7.1.3",
    "log-symbols": "^2.2.0"
  }
}
